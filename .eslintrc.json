{
	"env": {
		"node": true,
		"es2021": true
	},
	"extends": ["eslint:standard", "plugin:prettier/recommended"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"no-console": "warn",
		"no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
		"prefer-const": "error",
		"no-var": "error",
		"eqeqeq": ["error", "always"]
	}
}
